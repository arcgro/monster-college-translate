translate CUSTOM sc_07_eaf060a8:

    # mc "(Dad could have prepared me better.)"
    mc "(Papai poderia ter me preparado melhor.)"

# game/script/cap_01/sc_07.rpy:16
translate CUSTOM sc_07_8f7b0a5a:

